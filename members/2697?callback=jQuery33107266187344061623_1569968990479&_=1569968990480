ries: Meredith memorial lectures = Conférences commémoratives Meredith; 1981.","SortingTitle":"A bird's eye view of conflicting claims","Pages":"p. 88-112.","Issue":null,"Classification":null,"LanguageEn":"English","LanguageFr":"Anglais","ParticipationTypeId":1,"ParticipationTypeEn":"Sole Author","ParticipationTypeFr":"Auteur unique","OtherAuthors":null,"Publisher":"R. De Boo","PublisherLocation":"Don Mills, Ont.","OtherPublisher":null,"ISBN":null,"ISSN":null,"Publication":"In: McGill University. Faculty of Law. New developments in commercial lending = Les tendances actuelles en matière de financement commercial","PublicationPeriod":"1982","DateOfPublication":"0001-01-01T00:00:00Z","URL":null,"IsEditor":false,"IsDefault":true,"MatchingItems":[{"BibliographyItemId":8791,"Title":"Le créancier garanti face au syndic"}]},{"BibliographyItemId":7582,"DocumentTypeId":4,"DocumentTypeEn":"Articles","DocumentTypeFr":"Articles","Title":"Superintendent's levy may be payable on proceeds of realization","Series":null,"SubTitle":null,"SortingTitle":"Superintendent's levy may be payable on proceeds of realization","Pages":"15:89-91","Issue":"15:89-91","Classification":null,"LanguageEn":"English","LanguageFr":"Anglais","ParticipationTypeId":1,"ParticipationTypeEn":"Sole Author","ParticipationTypeFr":"Auteur unique","OtherAuthors":null,"Publisher":null,"PublisherLocation":null,"OtherPublisher":null,"ISBN":null,"ISSN":null,"Publication":"Legal Alert","PublicationPeriod":"1997","DateOfPublication":"0001-01-01T00:00:00Z","URL":null,"IsEditor":false,"IsDefault":true,"MatchingItems":[{"BibliographyItemId":5127,"Title":"BIA amendment increased compensation for landlords"}]},{"BibliographyItemId":7674,"DocumentTypeId":9,"DocumentTypeEn":"Books","DocumentTypeFr":"Livres","Title":"Contrats commerciaux : DRT-3358","Series":null,"SubTitle":"Note : Université de Montréal, Faculté de droit, 1980-1981.","SortingTitle":"Contrats commerciaux : DRT-3358","Pages":"55 p","Issue":null,"Classification":n